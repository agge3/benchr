#include <iostream>
#include <vector>
#include <algorithm>

int fib(int n) {
    if (n <= 1) return n;
    return fib(n-1) + fib(n-2);
}

int main() {

    std::cout << "Fib(25): " << fib(25) << std::endl;

    int count = 0;
    for (int num = 2; num < 10000; num++) {
        bool is_prime = true;
        for (int i = 2; i * i <= num; i++) {
            if (num % i == 0) {
                is_prime = false;
                break;
            }
        }
        if (is_prime) count++;
    }
    std::cout << "Primes under 10000: " << count << std::endl;
   
    long long sum = 0;
    for (int i = 0; i < 1000000; i++) {
        sum += i;
    }
    std::cout << "Sum: " << sum << std::endl;
    
    return 0;
}
